
<main class="survey-container">
	<div class="title-container">
		<h1>{{title}}</h1>
	</div>
	<article class="survey">
		<div class="questions-container">
			{{#each questions}}
			    <p class="question-text">
        			{{q}}
        		</p>
        		<input type="radio" name="Q1" value=1>YES
                <input type="radio" name="Q1" value=1>NO
			{{/each}}
		</div>
	</article>
</main>
