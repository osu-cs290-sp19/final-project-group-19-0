<main class="survey-container1">
	<article class="survey">
		<div class="title-container">
			<h1>{{title}}</h1>
		</div>
		<div class="questions-container">
			{{#each questions}}
			    <p class="question-text">
        			{{q}}
        		</p>
        		<div id="yes-text">
        			<p class="question-yes">
        				Yes: {{y}}
        			</p>
        		</div>
        		<div id="no-text">
	        		<p class="question-no">
	        			No: {{n}}
	        		</p>
	        	</div>
			{{/each}}
		</div>
	</article>
</main>